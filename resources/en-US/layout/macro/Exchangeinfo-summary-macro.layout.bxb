layout-macro-def(Exchangeinfo-summary-macro) {
  params {
    param (exchangeinfo) {
      type (Exchangeinfo)
      min (Required)
      max (One)
    }
  }

  content {
    
    cell-card {
      slot2 {
          content {
              order (PrimarySecondary)
              primary ("#{value (exchangeinfo.symbol)} #{value (exchangeinfo.exchange)}")
          }
      }
    }
  }
}
